.block
  .secondary-navigation
    %ul.wat-cf
      %li.first=link_to pat(:list), url(:media, :index)
      %li.active=link_to pat(:new), url(:media, :new)
  .content
    %h2.title
      =pat(:new)
      =mt(:media)
    .inner
      -form_for :media, url(:media, :create), :multipart => true, :class => :form do |f|
        =partial "medias/form", :locals => { :f => f }

-content_for :sidebar, partial("base/sidebar")