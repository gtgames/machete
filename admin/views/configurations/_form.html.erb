<div class="module_content">
  <%= f.input :_id,   :as => :string %>
  <%= f.input :val, :as => :string %>
</div>

<div class='submit_link'>
  <%= f.submit pat(:save), :class => :alt_btn %>
  <%= f.submit pat(:cancel), :onclick => "window.location='#{url(:configurations, :index)}';return false", :class => :button %>
</div>
