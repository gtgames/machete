%nav.secondary-navigation
  %ul.wat-cf
    %li.first.active=link_to pat(:list), url(:photos, :index)
    %li=link_to pat(:new), url(:photos, :new)
#content
  %h2
    =pat(:all)
    =mt(:photo)
  .inner
    %table
      %tr
        %th.first=mat(:partner, :name)
        %th=mat(:partner, :file)
        %th=mat(:partner, :tag_list)
        %th.last="&nbsp;"
      -@partner.each do |p|
        %tr
          %td.first=p.url
          %td
            %img{:src => "#{p.file.url}"}
          %td.last
            =button_to pat(:edit), url(:partner, :edit, :id => p.id), :method => :get, :class => :button_to
            =button_to pat(:delete), url(:partner, :destroy, :id => p.id), :method => :delete, :class => :button_to_delete
